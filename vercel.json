{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxDuration": 30,
          "includeFiles": [
            "requirements.txt",
            "static/**",
            "templates/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "FLASK_APP": "app.py"
    }
  }
  